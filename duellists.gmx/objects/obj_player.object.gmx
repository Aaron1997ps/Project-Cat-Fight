            <string>///Player Actions + Movement

onground = place_meeting(x,y+1,par_wall)

//Movement
if c_right[2]
    hspeed = movespeed
else if c_left[2]
    hspeed = -movespeed
else
    hspeed = 0

if onground and c_jump[1]
    vspeed = -jumpspeed

//Gravity
vspeed += grav

//Horizontal Wall Collisions
if place_meeting(x+hspeed,y,par_wall){
    while !place_meeting(x+sign(hspeed),y,par_wall){
        x+=sign(hspeed)
    }
    hspeed = 0
}

//Verticle Wall Collisions
if place_meeting(x,y+vspeed,par_wall){
    while !place_meeting(x,y+sign(vspeed),par_wall){
        y+=sign(vspeed)
    }
    vspeed = 0
}

//Reset the room
if keyboard_check(ord("R")){
    game_restart();
}

